<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.ease-crc.org/ont/SOMA.owl#"
     xml:base="http://www.ease-crc.org/ont/SOMA.owl"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:SOMA="http://www.ease-crc.org/ont/SOMA.owl#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://www.ease-crc.org/ont/SOMA-IO.owl">
        <owl:imports rdf:resource="http://www.ease-crc.org/ont/SOMA.owl"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#encodes -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#encodes">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith"/>
        <owl:inverseOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#isEncodedBy"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#InformationObject"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#InformationObject"/>
        <rdfs:comment xml:lang="en">The relation between two Information Objects that have the same meaning, but are formatted differently. E.g., a text written in UTF-8 encodes a text in a natural writing system (letters) and vice versa.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#givesMeaningTo -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#givesMeaningTo">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith"/>
        <owl:inverseOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#isGivenMeaningBy"/>
        <rdfs:domain rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#System"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#InformationObject"/>
        <rdfs:label xml:lang="en">The relation between a System and Information Object that is given meaning to by said system, e.g., a Language might give meaning to some word, sentence, text, etc., but without the knowledge of said System (Language), the text will not make sense to a reader.</rdfs:label>
        <rdfs:label xml:lang="en">gives meaning to</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#isEncodedBy -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#isEncodedBy">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith"/>
        <rdfs:comment xml:lang="en">The relation between two Information Objects that have the same meaning, but are formatted differently. E.g., a text written in UTF-8 is encoded by a text in a natural writing system (letters) and vice versa.</rdfs:comment>
        <rdfs:label xml:lang="en">is encoded By</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#isGivenMeaningBy -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#isGivenMeaningBy">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#InformationObject"/>
        <rdfs:range rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#System"/>
        <rdfs:comment xml:lang="en">The relation between an Information Object and a System that gives meaning to said object, e.g., a word, sentence, text, etc. might be given meaning by a Language and without the knowledge of said System (Language), the text will not make sense to a reader.</rdfs:comment>
        <rdfs:label xml:lang="en">is given meaning by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#expresses -->

    <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#expresses">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isAbout"/>
    </rdf:Description>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isExpressedBy -->

    <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isExpressedBy">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isReferenceOf"/>
    </rdf:Description>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#API_Specification -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/SOMA.owl#API_Specification">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#InterfaceSpecification"/>
        <rdfs:label xml:lang="en">API Specification</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#Algorithm -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/SOMA.owl#Algorithm">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Plan"/>
        <rdfs:comment xml:lang="en">An Algorithm is a finite sequence of well-defined instructions, typically used to solve a class of specific problems or to perform a computation.

From Wikipedia: https://en.wikipedia.org/wiki/Algorithm</rdfs:comment>
        <rdfs:label xml:lang="en">Algorithm</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#Archive -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/SOMA.owl#Archive">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.ease-crc.org/ont/SOMA.owl#Structured_Text"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#expresses"/>
                        <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#FileConfiguration"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#isGivenMeaningBy"/>
                <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#ArchiveFormat"/>
            </owl:Restriction>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#ArchiveFile -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/SOMA.owl#ArchiveFile">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.ease-crc.org/ont/SOMA.owl#Digital_File"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#realizes"/>
                        <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#Archive"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#Digital_File"/>
        <rdfs:comment xml:lang="en">An archive file is a computer file that is composed of one or more files along with metadata.

Source: https://en.wikipedia.org/wiki/Archive_file</rdfs:comment>
        <rdfs:label>Archive File</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#ArchiveFormat -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/SOMA.owl#ArchiveFormat">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#File_format"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#givesMeaningTo"/>
                <owl:allValuesFrom rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#Archive"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">An Archive Format is the file format of an archive file.

Source: https://en.wikipedia.org/wiki/Archive_file#Archive_formats</rdfs:comment>
        <rdfs:label xml:lang="en">Archive Format</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#Client-Server_Specification -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/SOMA.owl#Client-Server_Specification">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#InterfaceSpecification"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#definesRole"/>
                        <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#ClientRole"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#definesRole"/>
                        <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#ServerRole"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">Client-Server Specification</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#ClientRole -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/SOMA.owl#ClientRole">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#InterfaceComponentRole"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isDefinedIn"/>
                <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#Client-Server_Specification"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">Client Role</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#Computer_Code -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/SOMA.owl#Computer_Code">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.ease-crc.org/ont/SOMA.owl#Structured_Text"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#isGivenMeaningBy"/>
                        <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#Programming_Language"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.ease-crc.org/ont/SOMA.owl#Structured_Text"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#expresses"/>
                        <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#Algorithm"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment xml:lang="en">The Program itself (the specific set of instruction in a Programming Language), not the file that it is contained in nor the implemented algorithm!</rdfs:comment>
        <rdfs:label xml:lang="en">Computer Program</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#Computer_Language -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/SOMA.owl#Computer_Language">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#FormalLanguage"/>
        <rdfs:comment xml:lang="en">A computer language is a formal language used in communication with a computer.

From Wikipedia: https://en.wikipedia.org/wiki/Computer_language</rdfs:comment>
        <rdfs:label xml:lang="en">Computer Language</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#Digital_File -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/SOMA.owl#Digital_File">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#InformationRealization"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#realizes"/>
                <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#Structured_Text"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#hasNameString"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Any file that exists as a digital resource (but not its content), e.g., a text file actually laying on some hard drive, but not the contained text.</rdfs:comment>
        <rdfs:label xml:lang="en">Computer File</rdfs:label>
        <rdfs:label xml:lang="en">Digital File</rdfs:label>
    </owl:Class>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#Digital_File"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2000/01/rdf-schema#subClassOf"/>
        <owl:annotatedTarget>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#realizes"/>
                <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#Structured_Text"/>
            </owl:Restriction>
        </owl:annotatedTarget>
        <rdfs:comment xml:lang="en">The file content.</rdfs:comment>
    </owl:Axiom>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#Digital_File"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2000/01/rdf-schema#subClassOf"/>
        <owl:annotatedTarget>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#hasNameString"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </owl:annotatedTarget>
        <rdfs:comment xml:lang="en">The path of the file on a computer system.</rdfs:comment>
    </owl:Axiom>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#Executable -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/SOMA.owl#Executable">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.ease-crc.org/ont/SOMA.owl#Computer_Code"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#isGivenMeaningBy"/>
                        <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#ExecutableFormat"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#Computer_Code"/>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#ExecutableFile -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/SOMA.owl#ExecutableFile">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.ease-crc.org/ont/SOMA.owl#Digital_File"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#realizes"/>
                        <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#Executable"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:label xml:lang="en">Executable File</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#ExecutableFormat -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/SOMA.owl#ExecutableFormat">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#File_format"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#givesMeaningTo"/>
                <owl:allValuesFrom rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#Executable"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">Executable Format</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#ExecutableSoftware -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/SOMA.owl#ExecutableSoftware">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.ease-crc.org/ont/SOMA.owl#Software"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasMember"/>
                        <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#ExecutableFile"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:label xml:lang="en">Executable Software</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#Execution -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/SOMA.owl#Execution">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Process"/>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#FaultySoftware -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/SOMA.owl#FaultySoftware">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#SoftwareDiagnosis"/>
        <rdfs:comment>A description of a situation where some software has a bug.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#FileConfiguration -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/SOMA.owl#FileConfiguration">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Configuration"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasMember"/>
                        <owl:allValuesFrom rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#Digital_File"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment xml:lang="en">A configuration whose members are all files. Used to model, e.g., concrete collections of zip- and jar-files (and so on).</rdfs:comment>
        <rdfs:label xml:lang="en">File Configuration</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#File_format -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/SOMA.owl#File_format">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#Computer_Language"/>
        <rdfs:comment xml:lang="en">A File Format is a standard way that information is encoded for storage in a computer file. It specifies how bits are used to encode information in a digital storage medium.

From Wikipedia: https://en.wikipedia.org/wiki/File_format</rdfs:comment>
        <rdfs:label xml:lang="en">File Format</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#FormalLanguage -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/SOMA.owl#FormalLanguage">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#Language"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#givesMeaningTo"/>
                <owl:allValuesFrom rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#Structured_Text"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A Formal Language consists of words whose letters are taken from an alphabet and are well-formed according to a specific set of rules.

From Wikipedia: https://en.wikipedia.org/wiki/Formal_language</rdfs:comment>
        <rdfs:label xml:lang="en">Formal Language</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#HostRole -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/SOMA.owl#HostRole">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#InterfaceComponentRole"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isDefinedIn"/>
                <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#PluginSpecification"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">Host Role</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#Human-readable_Programming_Language -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/SOMA.owl#Human-readable_Programming_Language">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#Programming_Language"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#givesMeaningTo"/>
                <owl:allValuesFrom rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#Source_Code"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A Programming language like Java, Python etc. but not binary machine code.</rdfs:comment>
        <rdfs:label xml:lang="en">Human-readable Programming Language</rdfs:label>
    </owl:Class>
    


