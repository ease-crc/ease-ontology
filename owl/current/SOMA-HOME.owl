        <owl:inverseOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#isConceptIncludedIn"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Situation"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Concept"/>
        <rdfs:comment>A relation recording that a Situation has a Concept as participant in some sort of role.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA.owl"/>
        <rdfs:label>includes concept</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#includesRecord -->


    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#includesRecord">
